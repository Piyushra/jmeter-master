{
  "schedule": "R1//P10Y",
  "name": "jmeter-master","container": {
    "type": "DOCKER",
    "image": "jmeter-master-image",
"network":"BRIDGE",
"parameters":[
{
"key":"net",
"value":"weave"
}
],
    "volumes": [
      {
        "containerPath": "/logs",
        "hostPath": "/home/cloud-user/iotsp-jmeter/master/logs",
        "mode": "RW"
      }
    ]
},
"environmentVariables": [
        { "name" : "INPUT_FILE_NAME",
          "value": "sample.jmx"
},
        {"name":"OUTPUT_FILE_NAME", "value": "sample.jtl"},
{"name":"MARATHON_SERVER","value":"10.X.X.X"}
    ],

  "cpus": "0.5",
  "mem": "512",
  "instances":"1",
"command":"/MainScriptJmeter.sh"
}
